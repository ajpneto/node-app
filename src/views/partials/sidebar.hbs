      <div class="col-md-12">
         <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
           {{#each this}}{{#if category}}
	    <div {{#if @first}}hidden{{/if}} class="panel panel-default">
	       <div class="panel-heading" role="tab" id="headingOne{{category}}">
	          <h4 class="panel-title">
		     <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" 
                        href="#collapseOne{{category}}" aria-expanded="true" aria-controls="collapseOne{{category}}">
			{{replaceString category}}
		     </a>
		  </h4>
	       </div>
	       <div id="collapseOne{{category}}" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne{{category}}">
	          <div class="panel-body p-3">
                  <div class="row">
                  {{#each items}}
                    <div class="h6 fw-bolder"><a href="/lessons/{{key}}">{{title}}</a></div><hr>
                  {{/each}}
                  </div>
                  </div>
	       </div>
	    </div>{{/if}}
           {{/each}}
         </div>
      </div>
<center>
    <button class="btn btn-info" data-bs-toggle="modal" data-bs-target="#modal4">
      <strong class="text-white">View periodic table</strong>
    </button>
</center>
{{>modal4}}
