<script>

fetch('https://api.nobelprize.org/v1/laureate.json').then((response) => {
    response.json().then((data) => {
        if (data.error) {
            console.log(data.error)
        } else {
            data.laureates.forEach((item) => {

                str = `<div class="small text-muted">${item.born} - ${item.died}</div>
                          <h3>${item.firstname} ${item.surname}</h3>
                       </div>`

                $('.news').append(str);
            });
        }
    });
});

</script>
